{
  "name": "tescoTodo",
  "version": "0.0.1",
  "description": "Tesco Todo App",
  "scripts": {
    "start": "npm run server:production",
    "stop": "npm run clean",
    "server": "npm run build && npm run start-server",
    "watch": "npm run dev",
    "server:production": " npm run build:production && NODE_ENV=production forever start --uid 'tesco-todo' -a server/index.js",
    "dev": "rm -rf dist && forever stopall && webpack-dev-server --progress --config './webpack.config.dev-server.js'",
    "start-server": "nodemon server/index.js",
    "build": "npm run clean && webpack",
    "build:production": "npm run clean && webpack -p",
    "clean": "rm -rf dist && forever stopall",
    "test": "karma start",
    "test-dev": "karma start karma.dev.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nexmo/www-playground.git"
  },
